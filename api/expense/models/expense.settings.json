{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "expenses",
  "info": {
    "name": "expense"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "amount": {
      "type": "integer"
    },
    "project": {
      "model": "project",
      "via": "expenses"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "expenses"
    },
    "like": {
      "type": "integer"
    },
    "receipt": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "merchant_contact": {
      "type": "string"
    },
    "merchant_name": {
      "type": "string"
    },
    "confirm": {
      "type": "boolean"
    }
  }
}
