{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "expenses",
  "info": {
    "name": "expense"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "amount": {
      "type": "integer"
    },
    "project": {
      "model": "project",
      "via": "expenses"
    },
    "user": {
      "via": "expenses",
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
