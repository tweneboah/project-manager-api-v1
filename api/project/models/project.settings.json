{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "projects",
  "info": {
    "name": "project"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Education",
        "Business",
        "Estate",
        "Other"
      ]
    },
    "image": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "project_todos": {
      "collection": "project-todo",
      "via": "project"
    },
    "expenses": {
      "collection": "expense",
      "via": "project"
    },
    "incomes": {
      "collection": "income",
      "via": "project"
    },
    "user": {
      "via": "projects",
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
